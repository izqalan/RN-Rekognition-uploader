{
  "expo": {
    "name": "Face Indexer",
    "scheme": "uniten.faceindexer",
    "slug": "face-indexer",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/eye-90.png",
    "splash": {
      "image": "./assets/eye-90.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "updates": {
      "fallbackToCacheTimeout": 0
    },
    "assetBundlePatterns": ["**/*"],
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "dev.izqalan.uniten",
      "infoPlist": {
        "NSCameraUsageDescription": "This app uses the camera to index faces."
      }
    },
    "android": {
      "package": "dev.izqalan.uniten",
      "adaptiveIcon": {
        "foregroundImage": "./assets/eye-90.png",
        "backgroundColor": "#FFFFFF"
      }
    },
    "web": {
      "favicon": "./assets/favicon.png"
    }
  }
}
